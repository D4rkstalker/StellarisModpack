	attach = { "root" = "hexa_rays_entity" }
	attach = { "root1" = "penta_rays_entity" }
	attach = { "root1" = "hexa_rays_entity" }
	attach = { "root2" = "penta_rays_entity" }
	attach = { "root2" = "hexa_rays_entity" }
