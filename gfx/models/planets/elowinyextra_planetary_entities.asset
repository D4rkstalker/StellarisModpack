#disco moon#
entity = {
	name = "giga_disco_moon_base"
	pdxmesh = "planet_non_clouded_mesh"
	
	meshsettings = {
	name = "planet_geosphereShape"
	texture_diffuse = "giga_disco_moon_diffuse.dds"
	texture_normal = "gigabrown_dwarf_01_normal.dds"
	texture_specular = "giga_disco_moon_specular.dds"
	}
	
	meshsettings = {
	name = "polesShape"
	texture_diffuse = "poles_barren_1_diffuse.dds"
	texture_normal = "poles_barren_1_normal.dds"
	texture_specular = "poles_barren_1_specular.dds"
	}	
	
	playback_rate = 18
	
	default_state = "idle"
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class_02" } }
	}
}


entity = {
	name = "disco_rays_entity"
	locator = { name = "root"	position = { 0 	0  0 } }	
	locator = { name = "root1" position = { 0 0 0 } rotation = { 180 0 0 }}
	locator = { name = "root2" position = { 0 0 0 } rotation = { 0 180 0 }}
