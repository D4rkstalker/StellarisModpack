@ringworld_segment_size = 50

##################################################
############### MACHINE RINGWORLDS ###############
##################################################

entity = {
	name = "ringworld_machine_entity_01_entity"
	cull_radius = 500.0
	pdxmesh = "ringworld_habitable_01_mesh"
	
	default_state = "idle"
	state = { name = "idle" time_offset = { 0 100 }
		start_event = { trigger_once = yes sound = { soundeffect = "ringworld_hum_idle_01" } }
		start_event = { trigger_once = yes sound = { soundeffect = "ringworld_hum_idle_02" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class_02" } }
	}
	
	meshsettings = {
		name = "polySurface35Shape"	# Clouds
		texture_diffuse = "clouds_smog_01_diffuse.dds"
        texture_normal = "nonormal.dds"
        texture_specular = "nospec.dds"
		shader = "PdxMeshClouds"
	}
	
	meshsettings = {
		name = "polySurface34Shape"	# Cloud Shadows
		index = 0
		texture_diffuse = "clouds_smog_shadow_01_diffuse.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
		shader = "PdxMeshClouds"
	}
	
	meshsettings = {
		name = "pCube20Shape"	# Ringworld texture
		index = 0
		texture_diffuse = "ringworld_habitable_diffuse.dds"
		texture_normal = "ringworld_habitable_normal.dds"
		texture_specular = "ringworld_habitable_specular.dds"
		shader = "PdxMeshTerra"
	}

	meshsettings = {
		name = "polySurface9Shape"	# Planet texture
		index = 0
		texture_diffuse = "infested_diffuse.dds"
		texture_normal = "infested_normal.dds"
		texture_specular = "infested_spec.dds"
		shader = "PdxMeshTerra"
	}
	
	game_data = {
		size = @ringworld_segment_size
	}
	
	attach = { root = "ringworld_habitable_entity_01_clouds_entity" }
	attach = { root = "ringworld_cars_right_entity" }
	attach = { root = "ringworld_cars_left_entity" }
}


