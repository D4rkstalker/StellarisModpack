entity = {
	name = "biomass_01_dyson_sphere_stellaritering_entity"
	#pdxmesh = "dyson_sphere_part_1_mesh"
	scale = 1
		
	default_state = "idle"
	state = { name = "idle" state_time = 10
		start_event = { trigger_once = yes sound = { soundeffect = "dyson_sphere_bass_hum_partial" } }
	}
	
	locator = { name = "base3"				position = { 0	170  115 } rotation = {0 135 0} scale = 1}
	locator = { name = "base4"				position = { 0	170  -115 } rotation = {0 45 0} scale = 1}
	locator = { name = "base5"				position = { 0	-170  115 } rotation = {0 -135 0} scale = 1}
	locator = { name = "base6"				position = { 0	-170  -115 } rotation = {0 -45 0} scale = 1}
	
	attach = { "base3" = "stellarite_mass_section_entity" }
	attach = { "base4" = "stellarite_mass_section_entity" }
	attach = { "base5" = "stellarite_mass_section_entity" }
	attach = { "base6" = "stellarite_mass_section_entity" }

}

entity = {
	name = "biomass_01_biosphere_entity"
	pdxmesh = "planet_spherevanilla_mesh"
	
meshsettings = {
	name = "planet_geosphereShape"
	texture_diffuse = "infested_diffuse.dds"
	texture_normal = "infested_normal.dds"
	texture_specular = "infested_spec.dds"
	shader = "PdxMeshExtraDimensionalShip"
	}
	
	##pole texture override
	meshsettings = {
	name = "polesShape"
	texture_diffuse = "poles_1_diffuse.dds"
	texture_normal = "poles_1_normal.dds"
	texture_specular = "poles_1_specular.dds"
	shader = "PdxMeshExtraDimensionalShip"
	}
	
	##cloud texture override
	meshsettings = {
	name = "clouds_geosphereShape"
	texture_diffuse = "clouds_medium_diffuse.dds"
	texture_normal = "clouds_medium_normal.dds"
	texture_specular = "clouds_medium_specular.dds"
	shader = "PdxMeshExtraDimensionalShip"
	}
	
	##cloud shadow texture override
	meshsettings = {
	name = "clouds_shadow_geosphereShape"
	texture_diffuse = "clouds_medium_shadow_diffuse.dds"
	texture_normal = "nonormal.dds"
	texture_specular = "nospec.dds"
	shader = "PdxMeshExtraDimensionalShip"
	}
	
	default_state = "idle"
	state = { name = "idle" animation = "idle" time_offset = { 0 100 } }
}

