## ATTENTION - Spy orb destroyed entity has it's own scale ##
@spyorb_scale = 1.5 #DSS
@spyorb_height_pos = 0
@spyorb_side_pos = 0

animation = {
	name = "spyorb_part_1_idle"
	file = "spyorb_part_1_idle.anim"
}

animation = {
	name = "spyorb_part_2_idle"
	file = "spyorb_part_2_idle.anim"
}

animation = {
	name = "spyorb_part_3_idle"
	file = "spyorb_part_3_idle.anim"
}

entity = {
	name = "spy_orb_destroyed_entity"
	pdxmesh = "spy_orb_destroyed_01_mesh"	
	locator = { name = "debris"	position = { 0 0 0 } }	

	default_state = "idle"
	state = { name = "idle" state_time = 10
		start_event = { trigger_once = yes sound = { soundeffect = "spy_orb_destroyed" stop_on_state_change = yes } }
	}
		

		meshsettings = {
			name = "bodyShape"
			index = 0
			texture_diffuse = "spy_orb_destroyed_01_diffuse.dds"
			texture_normal = "spy_orb_destroyed_01_normal.dds"
			texture_specular = "spy_orb_destroyed_01_specular.dds"
			shader = "PdxMeshShip"
		}
		
		attach = { debris = "debris_small_01_entity" }
}


