entity = {
	name = "unbidden_04_military_station_xlarge_entity"
	pdxmesh = "unbidden_04_battle_fortress_mesh"
	
	locator = { name = "part1" 	position = { 0 0 0 } }
	locator = { name = "part2" 	position = { 0 0 0 } }
	locator = { name = "part3" 	position = { 0 0 0 } }
	locator = { name = "part4" 	position = { 0 0 0 } }
	locator = { name = "part5" 	position = { 0 0 0 } }
	locator = { name = "part6" 	position = { 0 0 0 } }
	locator = { name = "part7" 	position = { 0 0 0 } }
	locator = { name = "part8" 	position = { 0 0 0 } }

	default_state = "idle"
	state = { name = "idle" state_time = 5
		start_event = { trigger_once = yes sound = { soundeffect = "defense_station_idle_hum" } }
		event = { time = 0 particle = "ed_large_red_ship_effect"	keep_particle = yes trigger_once = yes }
		event = { time = 0.0 node = "light_locator_01" particle = "ship_light_red_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0.0 node = "light_locator_02" particle = "ship_light_red_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0.0 node = "light_locator_03" particle = "ship_light_red_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0.0 node = "light_locator_04" particle = "ship_light_red_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0.0 node = "light_locator_05" particle = "ship_light_red_effect" keep_particle = yes trigger_once = yes }
		
	}
	
	state = { name = "death" state_time = 5 looping = no
		event = { time = 0.0 node = "light_locator_01" particle = "ed_red_dissolve_effect" keep_particle = yes trigger_once = yes sound = { soundeffect = "station_explosion_small" } }
		event = { time = 0.5 node = "part3" particle = "ed_red_dissolve_effect" keep_particle = yes trigger_once = yes sound = { soundeffect = "station_explosion_small" } }
		event = { time = 1.5 node = "part6" particle = "ed_red_dissolve_effect" keep_particle = yes trigger_once = yes sound = { soundeffect = "station_explosion_small" } }
		event = { time = 2.0 node = "part1" particle = "ed_red_dissolve_effect" keep_particle = yes trigger_once = yes sound = { soundeffect = "station_explosion_small" } }
		event = { time = 3.0 node = "part3" particle = "ed_red_dissolve_effect" keep_particle = yes trigger_once = yes sound = { soundeffect = "station_explosion_small" } }
		event = { time = 4.0 node = "part7" particle = "ed_red_dissolve_effect" keep_particle = yes trigger_once = yes sound = { soundeffect = "station_explosion_small" } }
		
		event = { time = 4.9 particle = "medium_station_explosion_particle" keep_particle = yes trigger_once = yes sound = { soundeffect = "station_explosion_large" } }
	}
	
	scale = 0.4

	game_data = {
		size = @military_station_large
	}
}


