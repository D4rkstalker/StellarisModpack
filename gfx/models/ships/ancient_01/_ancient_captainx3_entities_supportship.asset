		event = { time = 0 node = "engine_medium_02" particle = "mammalian_01_1_45_exhaust_idle_particle" trigger_once = yes keep_particle = yes }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_event_ship_05_idle" } }
