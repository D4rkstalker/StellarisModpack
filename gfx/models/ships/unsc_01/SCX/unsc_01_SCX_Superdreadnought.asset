	}
	state = { name = "death" animation = "death3" animation_blend_time = 0.0 chance = 0.5 looping = no
		event = { time = 0.0 particle = "large_ship_explosion_particle" sound = { soundeffect = ship_destroyed } }
	}
	
	locator = { name = "exhaust_middle" position = { 0 -0.599 -29.7} rotation = { 180 0 0 } }
	locator = { name = "exhaust_left" position = { 2.759 -2.385 -26.5} rotation = { 180 0 0 } }
	locator = { name = "exhaust_right" position = { -2.759 -2.385 -26.5} rotation = { 180 0 0 } }
	
	game_data = {
		size = 40
	}
}

