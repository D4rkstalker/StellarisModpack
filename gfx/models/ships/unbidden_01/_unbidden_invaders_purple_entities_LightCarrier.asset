# FRAME

entity = {
	name = "unbidden_01_LightCarrier_entity"
	pdxmesh = "empty_mesh"

	locator = { name = "part1"	position = { 0 0.7 -13.6 } rotation = { 0 0 0 } }
	locator = { name = "part2"	position = { 0 0.7 -6.2 } rotation = { 0 0 0 } }
	locator = { name = "part3"	position = { 0 0.71 6.2 } rotation = { 0 0 0 } }
	locator = { name = "part4"	position = { 0 0.7 10.65 } rotation = { 0 0 0 } }
	
	default_state = "idle"
	state = { name = "idle"		state_time = 5
		event = { time = 0 particle = "ed_large_violet_ship_effect"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_01" particle = "extradimensional_01_violet_exhaust_idle"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_02" particle = "extradimensional_01_violet_exhaust_idle"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_03" particle = "extradimensional_01_violet_exhaust_idle"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_04" particle = "extradimensional_01_violet_exhaust_idle"	keep_particle = yes trigger_once = yes }
		start_event = { trigger_once = yes sound = { soundeffect = "ex_battleship_hum" } }
	}
	state = { name = "moving"		state_time = 5
		event = { time = 0 particle = "ed_large_violet_ship_effect"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_01" particle = "extradimensional_01_violet_exhaust_moving"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_02" particle = "extradimensional_01_violet_exhaust_moving"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_03" particle = "extradimensional_01_violet_exhaust_moving"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_04" particle = "extradimensional_01_violet_exhaust_moving"	keep_particle = yes trigger_once = yes }
	}
	state = { name = "death"		state_time = 5 
		event = { time = 0 node = "root" particle = "ed_violet_dissolve_effect"	keep_particle = yes trigger_once = yes }
	}
	
	game_data = {
		uv_animation_speed 		= 0.2
		uv_animation_direction 	= { 0.0 0.02 }
		bloom_factor = @exd_bloom_factor
		size = @LightCarrier_scale
	}
	
	scale = 0.8
}

