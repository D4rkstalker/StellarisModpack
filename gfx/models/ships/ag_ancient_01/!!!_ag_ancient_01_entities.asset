entity = {
	name = "ag_ancient_01_corvette_entity"
	pdxmesh = "ag_ancient_small_ship_frame_mesh"
	
	scale = 1.0
	default_state = "idle"
	state = { name = "idle"	 animation = "idle" animation_blend_time = 0.25
		start_event = { trigger_once = yes sound = { soundeffect = "ag_ancient_ship_small_idle" } }
	}
	state = { name = "moving"	animation = "idle" animation_blend_time = 0.25
		start_event = { trigger_once = yes sound = { soundeffect = "ag_ancient_ship_small_idle" } }
		event = { trigger_once = yes sound = { soundeffect = moving_out_corvette } }
	}
	state = { name = "death" animation = "death1" animation_blend_time = 0.0 chance = 1 looping = no
		event = { time = 4.6 node = "root" particle = "ag_ship_explosion_l_red" keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
	}
	state = { name = "death" animation = "death2" animation_blend_time = 0.0 chance = 1 looping = no
		event = { time = 4.6 node = "root" particle = "ag_ship_explosion_l_red" keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
	}
	state = { name = "death" animation = "death3" animation_blend_time = 0.0 chance = 1 looping = no
		event = { time = 4.6 node = "root" particle = "ag_ship_explosion_l_red" keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
	}
	game_data = { size = @ag_corvette_size }
}
