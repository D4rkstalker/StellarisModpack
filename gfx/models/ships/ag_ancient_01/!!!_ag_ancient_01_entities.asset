entity = {
	name = "ag_ancient_01_destroyer_entity"
	pdxmesh = "ag_ancient_01_destroyer_frame_mesh"
	
	scale = @ag_destroyer_scale
	
	locator = { name = "root" 	position = { 0 0 @ag_scaled_sphere_loc } }
	attach = { root = "ag_ancient_01_sphere_entity" }

	default_state = "idle"
	state = { name = "idle" animation = "idle" animation_blend_time = 0
		start_event = { trigger_once = yes sound = { soundeffect = "ag_ancient_ship_small_idle" } }
	}
	state = { name = "combat_idle"		animation = "attack" animation_blend_time = 0
		start_event = { trigger_once = yes sound = { soundeffect = "ag_ancient_ship_small_idle" } }
		start_event = { trigger_once = yes sound = { soundeffect = "ag_ancient_ship_combat_idle" stop_on_state_change = yes } }
	}
	state = { name = "combat_moving"		animation = "attack" animation_blend_time = 0
		start_event = { trigger_once = yes sound = { soundeffect = "ag_ancient_ship_small_idle" } }
		start_event = { trigger_once = yes sound = { soundeffect = "ag_ancient_ship_combat_idle" stop_on_state_change = yes } }
	}
	state = { name = "moving"		animation = "idle" animation_blend_time = 0
		start_event = { trigger_once = yes sound = { soundeffect = "ag_ancient_ship_small_idle" } }
		event = { trigger_once = yes sound = { soundeffect = "moving_out_destroyer" stop_on_state_change = yes } }
	}
	state = { name = "death" animation = "idle"		looping = no
	    event = { time = 0.2 node = "explode_loc_4"      particle = "ag_ship_explosion_m_red"       trigger_once = yes keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
	    event = { time = 0.5 node = "explode_loc_2"      particle = "ag_ship_explosion_m_red"       trigger_once = yes keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
	    event = { time = 0.7 node = "explode_loc_1"      particle = "ag_ship_explosion_m_red"       trigger_once = yes keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
	    event = { time = 1.0 node = "explode_loc_6"      particle = "ag_ship_explosion_m_red"       trigger_once = yes keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
	    event = { time = 1.2 node = "explode_loc_2"      particle = "ag_ship_explosion_m_red"       trigger_once = yes keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
	    event = { time = 1.5 node = "explode_loc_5"      particle = "ag_ship_explosion_m_red"       trigger_once = yes keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
	    event = { time = 1.7 node = "primary_weapon"      particle = "ag_ship_explosion_m_red"       trigger_once = yes keep_particle = yes sound = { soundeffect = "station_explosion_small" } }
		event = { time = 1.9 node = "root"      particle = "ag_ship_explosion_l_red" keep_particle = yes trigger_once = yes sound = { soundeffect = "station_explosion_energy" } }
	}
	game_data = { size = @ag_destroyer_size emissive_recolor_crunch = 3.0 }
}
