# FRAME

entity = {
	name = "c_plantoid_03_Dreadnought_entity"
	pdxmesh = "c_plantoid_03_battleship_frame_mesh"

	locator = { name = "part1"	position = { 0 0 -10.0 } rotation = { 0 0 0 } }
	locator = { name = "part2"	position = { 0 0 -5.5 } rotation = { 0 0 0 } }
	locator = { name = "part3"	position = { 0 0 5.5 } rotation = { 0 0 0 } }
	locator = { name = "part4"	position = { 0 0 12.0 } rotation = { 0 0 0 } }
	
	default_state = "idle"
	state = { name = "idle"	animation = "idle" animation_blend_time = 4.0 
		start_event = { trigger_once = yes sound = { soundeffect = "amb_battleship_hum" } }
	}
	state = { name = "moving"	animation = "idle" animation_blend_time = 4.0 
		event = { trigger_once = yes sound = { soundeffect = "moving_out_battleship" } }
