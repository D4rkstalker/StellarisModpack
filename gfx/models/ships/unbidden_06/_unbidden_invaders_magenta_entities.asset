entity = {
	name = "unbidden_06_corvette_entity"
	pdxmesh = "unbidden_06_warship_small_mesh"
	scale = 0.5
	locator = { name = "part1" }
	
	default_state = "idle"
	state = { name = "idle"		state_time = 5
		event = { time = 0 particle = "ed_small_magenta_ship_effect"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_01" particle = "extradimensional_01_magenta_exhaust_idle"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_02" particle = "extradimensional_01_magenta_exhaust_idle"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_large_01" particle = "extradimensional_01_magenta_exhaust_idle"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_large_02" particle = "extradimensional_01_magenta_exhaust_idle"	keep_particle = yes trigger_once = yes }
		start_event = { trigger_once = yes sound = { soundeffect = "ex_corvette_hum" } }
	}
	state = { name = "moving"		state_time = 5
		event = { time = 0 particle = "ed_small_magenta_ship_effect"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_01" particle = "extradimensional_01_magenta_exhaust_moving"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_medium_02" particle = "extradimensional_01_magenta_exhaust_moving"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_large_01" particle = "extradimensional_01_magenta_exhaust_moving"	keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "engine_large_02" particle = "extradimensional_01_magenta_exhaust_moving"	keep_particle = yes trigger_once = yes }
	}
	state = { name = "death"		state_time = 5 
		event = { time = 0 node = "root" particle = "ed_magenta_dissolve_effect"	keep_particle = yes trigger_once = yes }
	}

	game_data = {
		uv_animation_speed 		= 0.2
		uv_animation_direction 	= { 0.0 0.02 }
		bloom_factor = @exd_bloom_factor
	}
}

